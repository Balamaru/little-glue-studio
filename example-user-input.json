{
  "source": {
    "type": "postgresql",
    "table": "public.users",
    "host": "db.example.local",
    "port": 5432,
    "database": "mydb",
    "user": "myuser",
    "password": "mypassword"
  },
  "transforms": [
    { "type": "filter", "condition": "age > 25" },
    { "type": "select", "columns": ["id", "name", "age"] }
  ],
  "target": {
    "type": "s3",
    "path": "s3://my-bucket/output/users/"
  }
}
